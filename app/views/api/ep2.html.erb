<table>
  <tr>
  	<th>Id</th>
    <th>URL</th>
    <th>Content Type</th>
    <th>Content</th>
  </tr>

  <% @urls.each do |url| %>
  	<% url.contents.each do |content| %>
		<tr>
			<td><%= url.id %></td>
    	<td><%= url.url %></td>
	    <td><%= content.content_type %></td>
	    <td><%= content.content %></td>
	  </tr>
	  <% end %>
  <% end %>
</table>
